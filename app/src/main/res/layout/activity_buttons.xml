<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Buttons">

    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button5"
            style="@style/successTextMaterialButton"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginStart="8dp" app:layout_constraintStart_toEndOf="@+id/button2" android:enabled="true"/>
    <TextView
            android:text="Text - Error "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/button8" app:layout_constraintStart_toStartOf="@+id/guideline2"
            android:layout_marginStart="12dp" android:layout_marginTop="136dp"/>
    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button3"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            style="@style/errorTextMaterialButton"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="Text - Alert "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/button8"
            android:layout_marginStart="8dp" android:layout_marginTop="136dp"
            app:layout_constraintStart_toEndOf="@+id/textView"/>
    <TextView
            android:text="Text - Primary "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/button8" app:layout_constraintStart_toStartOf="@+id/guideline2"
            android:layout_marginStart="8dp" android:layout_marginTop="16dp"/>
    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button2"
            style="@style/alertTextMaterialButton"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/button3"/>
    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/sd_left_button"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            style="@style/textMaterialButton"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp"/>
    <TextView
            android:text="Text - Alert "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/button8" app:layout_constraintStart_toStartOf="@+id/button2"
            android:layout_marginStart="8dp" android:layout_marginTop="136dp"/>
    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button7"
            app:layout_constraintStart_toStartOf="@+id/textView16"
            style="@style/materialButton"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView16"
            android:enabled="false"/>
    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button4"
            style="@style/materialButton"
            app:layout_constraintStart_toStartOf="@+id/guideline"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView18"
            android:layout_marginStart="8dp"/>
    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button8"
            app:layout_constraintStart_toStartOf="@+id/guideline2"
            style="@style/outlineMaterialButton"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView19"
            android:enabled="false" android:layout_marginStart="8dp" android:textAppearance="@style/Button"/>
    <com.google.android.material.button.MaterialButton
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button9"
            style="@style/outlineMaterialButton"
            app:layout_constraintStart_toStartOf="@+id/guideline"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView20"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="Contained On - Primary "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView18"
            app:layout_constraintTop_toBottomOf="@+id/textView17" android:layout_marginTop="24dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintStart_toStartOf="@+id/guideline"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="Contained Off "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16"
            app:layout_constraintTop_toBottomOf="@+id/textView17" android:layout_marginTop="24dp"
            app:layout_constraintStart_toStartOf="@+id/guideline2" android:layout_marginStart="8dp"
    />

    <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="34dp"
            android:layout_marginTop="16dp"
            android:text="Buttons - 15px "
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    <TextView
            android:text="Outline Off "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView19" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/button7" app:layout_constraintStart_toStartOf="@+id/guideline2"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="Outline On - Primary "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView20"
            app:layout_constraintTop_toTopOf="@+id/textView19"
            app:layout_constraintStart_toStartOf="@+id/guideline" android:layout_marginStart="8dp"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline"
                                                android:orientation="vertical"
                                                app:layout_constraintGuide_percent="0.5"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline2" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="vertical"/>
</androidx.constraintlayout.widget.ConstraintLayout>